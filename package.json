{
    "name": "gotolastedit",
    "displayName": "Goto Last Edit",
    "description": "Goes backward and forward in the last edited location history",
    "version": "0.0.1",
    "publisher": "plrenaudin",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.goBack",
                "title": "Go To Last Edited Location"
            },
            {
                "command": "extension.goForward",
                "title": "Go To Next Edited Location"
            }
        ],
        "keybindings": [
            {
                "command": "extension.goBack",
                "key": "ctrl+-",
                "mac": "cmd+-",
                "when": "editorTextFocus"
            },{
                "command": "extension.goForward",
                "key": "ctrl+=",
                "mac": "cmd+=",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}